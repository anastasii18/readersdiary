<h2>
  <div class="lists">Добавить книгу в список</div>
</h2>
<div class="titles">
  <%= form_for :list do |f| %>
    <div class="field"> <%= f.collection_select :list_id, List.all, :id, :list_name %><br/></div>
    <div class="field"> <%= f.collection_select :book_id, Book.all, :id, :title %><br/></div>
    <div class="actions"><%= f.submit %></div>
  <% end %>
</div>